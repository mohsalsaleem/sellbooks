<%= simple_form_for(@refer) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :email %>
    <%= f.input :registered_on %>
    <%= f.input :phnumber %>
    <%= f.input :refid %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
